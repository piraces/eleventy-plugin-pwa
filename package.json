    "repository": {
        "type": "git",
        "url": "git+https://github.com/piraces/eleventy-plugin-pwa.git"
    },
    "keywords": [
        "eleventy-plugin",
        "eleventy",
        "workbox",
        "pwa"
    ],
    "author": {
        "name": "Raúl Piracés",
        "email": "raul@piraces.dev",
        "url": "https://piraces.dev/"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/piraces/eleventy-plugin-pwa/issues"
    },
    "homepage": "https://github.com/piraces/eleventy-plugin-pwa#readme",
    "dependencies": {
        "shimmer": "^1.2.1",
        "workbox-build": "^6.1.5"
    },
    "devDependencies": {
        "@11ty/eleventy": "^0.12.1",
        "jest": "^27.0.5",
        "rimraf": "^3.0.2"
    },
    "jest": {
